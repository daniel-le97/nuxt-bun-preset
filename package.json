{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun --bun nuxt build",
    "dev": "bun --bun nuxt dev",
    "start": "bun .output/server/index.mjs",
    "generate": "nuxt generate",
    "postinstall": "bun --bun nuxt prepare",
    "lint": "bun --bun eslint .",
    "lint:fix": "bun --bun eslint . --fix",
    "kill:port": "PORT=$1 | kill -9 $(lsof -t -i :$PORT)",
    "reset:db": "rm db.sqlite*",
    "bench": "bombardier -c 125 -n 100000 http://localhost:3000"
  },
  "dependencies": {
    "@auth/core": "^0.20.0",
    "immer": "^10.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@nuxt/devtools": "^1.0.6",
    "@nuxt/ui": "latest",
    "@types/bun": "^1.0.0",
    "@vueuse/nuxt": "latest",
    "eslint": "^8.56.0",
    "nuxt": "npm:nuxt-nightly@latest",
    "vue": "^3.4.5",
    "vue-router": "^4.2.5"
  }
}
